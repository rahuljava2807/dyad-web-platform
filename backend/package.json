{
  "name": "@dyad/backend",
  "version": "1.0.0",
  "description": "Express.js backend for Dyad Web Platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx src/simple-server.ts",
    "dev:watch": "tsx watch src/simple-server.ts",
    "dev:full": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:load": "k6 run tests/load/basic.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:studio": "prisma studio",
    "logs": "tail -f logs/app.log",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.2",
    "@ai-sdk/azure": "^1.0.9",
    "@ai-sdk/google": "^1.0.2",
    "@ai-sdk/openai": "^1.0.2",
    "@aws-sdk/client-s3": "^3.698.0",
    "@aws-sdk/s3-request-presigner": "^3.698.0",
    "@azure/storage-blob": "^12.27.0",
    "@prisma/client": "^6.2.1",
    "ai": "^3.4.32",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.3",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.18",
    "openai": "^4.91.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.19",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.1",
    "playwright": "^1.56.0",
    "prisma": "^6.2.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "prisma": {
    "seed": "tsx src/scripts/seed.ts"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
